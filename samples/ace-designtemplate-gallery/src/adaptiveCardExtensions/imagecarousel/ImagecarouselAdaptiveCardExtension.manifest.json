{
  "$schema": "https://developer.microsoft.com/json-schemas/spfx/client-side-web-part-manifest.schema.json",
  "id": "a0f6e5f5-fd50-4e0b-97d7-12357841ee57",
  "alias": "ImagecarouselAdaptiveCardExtension",
  "componentType": "AdaptiveCardExtension",

  // The "*" signifies that the version should be taken from the package.json
  "version": "*",
  "manifestVersion": 2,

  // If true, the component can only be installed on sites where Custom Script is allowed.
  // Components that allow authors to embed arbitrary script code should set this to true.
  // https://support.office.com/en-us/article/Turn-scripting-capabilities-on-or-off-1f2c515f-5d7e-448a-9fd7-835da935584f
  "requiresCustomScript": false,
  "supportedHosts": ["Dashboard"],
  "supportsThemeVariants": true,
  "preconfiguredEntries": [{
    "groupId": "bd067b1e-3ad5-4d5d-a5fe-505f07d7f59c", // Dashboard
    "group": {
      "default": "Dashboard"
    },
    "title": {
      "default": "ACE - Image Carousel"
    },
    "description": {
      "default": "Design template for displaying images in a carousel with product data."
    },
    "iconImageUrl": "https://raw.githubusercontent.com/pnp/spfx-reference-scenarios/main/samples/ace-designtemplate-gallery/images/acepnplogo.png",
    "properties": {
      "title": "Devices",
      "cardHeading": "HoloLens 2",
      "description": "For precise, efficient hands-free work",
      "iconProperty": "https://raw.githubusercontent.com/pnp/spfx-reference-scenarios/main/samples/ace-designtemplate-gallery/images/imagecarousel/img-hololens-2.jpg" // Default to sharepointlogo
    }
  }]
}